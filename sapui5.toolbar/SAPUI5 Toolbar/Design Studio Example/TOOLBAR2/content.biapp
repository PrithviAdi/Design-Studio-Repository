			<bi:property name="onclick">
				<bi:value><![CDATA[var buttonPressed = TOOLBARMENU_1.getButtonClickedIndex();  // If you like using indexes.
var buttonLabel = TOOLBARMENU_1.getButtonClickedTitle();

// All code in one script window instead of multiple buttons

if(buttonLabel == "Variables"){ // Open Dialog
	APPLICATION.openPromptDialog(400, 300);
}
if(buttonLabel == "Open Query"){ // Open Dialog
	POPUP_1.show();
}
if(buttonLabel == "Show Filters"){ // Filter Panel
	FILTERPANEL_1.setVisible(true);
	TOOLBARMENU_1.setProperty(2, "icon", "data:image/gif;base64,");
	TOOLBARMENU_1.setProperty(2, "title", "Hide Filters");
}
if(buttonLabel == "Hide Filters"){
	FILTERPANEL_1.setVisible(false);
	TOOLBARMENU_1.setProperty(2, "icon", "data:image/gif;base64,R0lGODlhEAAPAPEAAMDAwADgAgcHBwAAACH5BAEAAAAALAAAAAAQAA8AAQIohI9pwu2+gpxUiljzxdkm0QXbpykHODUmNo6keK5oK1tqDa/nrfdAAQA7");
	TOOLBARMENU_1.setProperty(2, "title", "Show Filters");
}
if(buttonLabel == "Toggle Captions"){
	if(TOOLBARMENU_1.getCaptionVisibility()=="X"){
		TOOLBARMENU_1.setCaptionVisibility("");	
	}else{
		TOOLBARMENU_1.setCaptionVisibility("X");
	}
}]]></bi:value>
